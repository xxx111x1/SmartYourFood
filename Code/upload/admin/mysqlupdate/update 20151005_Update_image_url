update smartyourfood.oc_restaurant_info set img_url = './catalog/view/theme/default/image/foodImages/umi_unagi_bento.png' where img_url is null and restaurant_id >= 0;
update smartyourfood.oc_food set img_url = './catalog/view/theme/default/image/foodImages/umi_unagi_bento.png' where food_id = 255;